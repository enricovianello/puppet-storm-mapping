<%-
@pools.each do | pool |
  @vo_name=pool['vo']
  @pool_name=pool['name']
  @role=pool['role']
  @role1 = @role == nil ?  "/Role=NULL" : "/Role=#{@role}"
  @role2 = @role == nil ?  "" : "/Role=#{@role}"
  @capability=pool['capability']
  @capability1 = @capability == nil ? "/Capability=NULL" : "/Capability=#{@capability}"
  @capability2 = @capability == nil ? "" : "/Capability=#{@capability}"
  @line1 = "\"/#{@vo_name}#{@role1}#{@capability1}\" .#{@pool_name}"
  @line2 = "\"/#{@vo_name}#{@role2}#{@capability2}\" .#{@pool_name}"
  if (@line1 == @line2)
-%>
<%=@line1%>
<%-
  else
-%>
<%=@line1%>
<%=@line2%>
<%-
  end
end
-%>